{
  "name": "chained-notes-web",
  "version": "0.0.0",
  "description": "A web interface for chained notes",
  "repository": "https://github.com/msga-mmm/chained-notes-web/",
  "homepage": "https://msga-mmm.github.io/chained-notes-web/",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev-refresh": "cross-env USE_REFRESH=true vite",
    "build": "vite build",
    "build-refresh": "cross-env USE_REFRESH=true vite build",
    "preview": "vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix && knip --fix",
    "check": "tsc && bun run lint && bun run check:unused-code && bun run format:check",
    "format:check": "prettier --check .",
    "format": "prettier --write .",
    "test": "vitest src",
    "test:ui": "vitest src --ui",
    "coverage": "vitest run --coverage src",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "check:unused-code": "knip"
  },
  "dependencies": {
    "@auth0/auth0-react": "^2.2.4",
    "@faker-js/faker": "^8.4.1",
    "@lexical/react": "^0.17.0",
    "@reduxjs/toolkit": "^2.2.5",
    "classnames": "^2.5.1",
    "lexical": "^0.17.0",
    "nanoid": "^5.0.7",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.24.0"
  },
  "devDependencies": {
    "@eslint/compat": "^1.1.0",
    "@eslint/js": "^9.5.0",
    "@playwright/test": "^1.45.0",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^20.14.8",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.14.1",
    "@unocss/preset-attributify": "^0.61.0",
    "@unocss/preset-icons": "^0.61.0",
    "@unocss/preset-uno": "^0.61.0",
    "@unocss/reset": "^0.61.0",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/coverage-v8": "^1.6.0",
    "@vitest/ui": "^1.6.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.5.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-deprecation": "^3.0.0",
    "eslint-plugin-functional": "^6.6.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-promise": "^6.2.0",
    "eslint-plugin-react": "^7.34.3",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-testing-library": "^6.2.2",
    "globals": "^15.9.0",
    "happy-dom": "^15.0.0",
    "knip": "^5.27.3",
    "prettier": "^3.3.3",
    "type-fest": "^4.25.0",
    "typescript": "^5.5.4",
    "typescript-eslint": "^7.14.1",
    "unocss": "^0.61.0",
    "vite": "^5.4.2",
    "vite-tsconfig-paths": "^5.0.1",
    "vitest": "^2.0.5",
    "web-vitals": "^4.2.3"
  }
}
