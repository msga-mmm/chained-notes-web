{
  "name": "chained-notes-web",
  "version": "0.0.0",
  "description": "A web interface for chained notes",
  "repository": "https://github.com/msga-mmm/chained-notes-web/",
  "homepage": "https://msga-mmm.github.io/chained-notes-web/",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev-refresh": "cross-env USE_REFRESH=true vite",
    "build": "vite build",
    "build-refresh": "cross-env USE_REFRESH=true vite build",
    "preview": "vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix && knip --fix",
    "check": "tsc && bun run lint && bun run check:unused-code && bun run format:check",
    "format:check": "prettier --check .",
    "format": "prettier --write .",
    "test": "vitest src",
    "test:ui": "vitest src --ui",
    "coverage": "vitest run --coverage src",
    "test:e2e": "playwright test",
    "check:unused-code": "knip"
  },
  "dependencies": {
    "@lexical/react": "^0.14.2",
    "@reduxjs/toolkit": "^2.2.2",
    "classnames": "^2.5.1",
    "lexical": "^0.14.2",
    "nanoid": "^5.0.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^9.1.0",
    "react-router-dom": "^6.22.3"
  },
  "devDependencies": {
    "@eslint/js": "^8.57.0",
    "@playwright/test": "^1.42.1",
    "@testing-library/react": "^14.2.2",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^20.12.2",
    "@types/react": "^18.2.73",
    "@types/react-dom": "^18.2.23",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "@unocss/preset-attributify": "^0.58.8",
    "@unocss/preset-icons": "^0.58.8",
    "@unocss/preset-uno": "^0.58.8",
    "@unocss/reset": "^0.58.8",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.4.0",
    "@vitest/ui": "^1.4.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-deprecation": "^2.0.0",
    "eslint-plugin-functional": "^6.3.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-testing-library": "^6.2.0",
    "globals": "^15.0.0",
    "happy-dom": "^14.3.9",
    "knip": "^5.7.0",
    "prettier": "^3.2.5",
    "type-fest": "^4.14.0",
    "typescript": "^5.4.3",
    "typescript-eslint": "^7.4.0",
    "unocss": "^0.58.8",
    "vite": "^5.2.7",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^1.4.0",
    "web-vitals": "^3.5.2"
  }
}
