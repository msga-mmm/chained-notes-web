{
  "name": "chained-notes-web",
  "version": "0.0.0",
  "description": "A web interface for chained notes",
  "repository": "https://github.com/msga-mmm/chained-notes-web/",
  "homepage": "https://msga-mmm.github.io/chained-notes-web/",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev-refresh": "cross-env USE_REFRESH=true vite",
    "build": "vite build",
    "build-refresh": "cross-env USE_REFRESH=true vite build",
    "preview": "vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix && knip --fix",
    "check": "bun run check:types && bun run lint && bun run check:unused-code && bun run format:check",
    "check:unused-code": "knip",
    "check:types": "tsc",
    "format:check": "prettier --check .",
    "format": "prettier --write .",
    "test": "vitest src",
    "test:ui": "vitest src --ui",
    "coverage": "vitest run --coverage src",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:firefox": "playwright test --project firefox",
    "test:e2e:firefox:ui": "playwright test --project firefox --ui",
    "test:e2e:chrome": "playwright test --project chrome",
    "test:e2e:chrome:ui": "playwright test --project chrome --ui",
    "test:e2e:webkit": "playwright test --project webkit",
    "test:e2e:webkit:ui": "playwright test --project webkit --ui",
    "generate-openapi-client": "orval"
  },
  "dependencies": {
    "@auth0/auth0-react": "^2.2.4",
    "@lexical/react": "^0.28.0",
    "@tanstack/query-core": "^5.54.1",
    "@tanstack/react-query": "^5.55.0",
    "axios": "^1.7.7",
    "classnames": "^2.5.1",
    "lexical": "^0.28.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router": "^7.0.2"
  },
  "devDependencies": {
    "@eslint/compat": "^1.1.1",
    "@eslint/js": "^9.10.0",
    "@faker-js/faker": "^9.0.0",
    "@playwright/test": "^1.47.0",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^22.5.4",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@typescript-eslint/eslint-plugin": "^8.4.0",
    "@typescript-eslint/parser": "^8.4.0",
    "@unocss/preset-attributify": "^65.0.0",
    "@unocss/preset-icons": "^65.0.0",
    "@unocss/preset-uno": "^65.0.0",
    "@unocss/reset": "^65.0.0",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/coverage-v8": "^3.0.0",
    "@vitest/ui": "^3.0.0",
    "ajv": "^8.17.1",
    "cross-env": "^7.0.3",
    "eslint": "^9.10.0",
    "eslint-config-prettier": "^10.0.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-deprecation": "^3.0.0",
    "eslint-plugin-functional": "^9.0.0",
    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-jsx-a11y": "^6.10.0",
    "eslint-plugin-promise": "^7.1.0",
    "eslint-plugin-react": "^7.35.2",
    "eslint-plugin-react-hooks": "^5.1.0-rc-a03254bc-20240905",
    "eslint-plugin-testing-library": "^7.0.0",
    "globals": "^16.0.0",
    "happy-dom": "^17.0.0",
    "knip": "^5.30.0",
    "msw": "^2.4.3",
    "orval": "^7.1.0",
    "prettier": "^3.3.3",
    "type-fest": "^4.26.0",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.4.0",
    "unocss": "^65.0.0",
    "vite": "^5.4.3",
    "vite-tsconfig-paths": "^5.0.1",
    "vitest": "^3.0.0",
    "web-vitals": "^4.2.3"
  }
}
